{
  "extension": {
    "teamsapp": {
      "master_key": null,
      "bot_domain": null
    },
    "teamsagent": {
      "secret": null
    }
  },
  "evals": {
    "models": ["gpt-4o"],
    "judge_model": "gpt-5"
  },
  "service": {
    "agent": {
      "assistant": "knowledge_graph_agent",
      "model": "gpt-4o"
    },
    "llm": {},
    "tool": {
      "filter": {
        "threshold": 0.48,
        "max_tokens": null
      },
      "endpoints": {
        "get_ontology": {
          "method": "graph.Graph.GetOntology",
          "request": "common.Empty"
        },
        "get_subjects": {
          "method": "graph.Graph.GetSubjects",
          "request": "graph.SubjectsQuery"
        },
        "query_by_pattern": {
          "method": "graph.Graph.QueryByPattern",
          "request": "graph.PatternQuery"
        },
        "search_content": {
          "method": "vector.Vector.SearchContent",
          "request": "vector.TextQuery"
        }
      }
    }
  }
}
